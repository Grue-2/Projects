<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="anchor" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.StartScene">
   <children>
      <StackPane prefHeight="150.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <children>
            <ImageView fitHeight="900.0" fitWidth="1600.0" pickOnBounds="true" preserveRatio="true" smooth="false">
               <image>
                  <Image url="@background.png" />
               </image>
            </ImageView>
            <StackPane prefHeight="150.0" prefWidth="200.0">
               <children>
                  <BorderPane fx:id="borderPane" prefHeight="900.0" prefWidth="1600.0" style="-fx-background-image: url(&quot;../../background.png&quot;);">
                     <center>
                        <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="50.0" BorderPane.alignment="CENTER">
                           <children>
                              <Button mnemonicParsing="false" text="Button" visible="false" />
                              <Button fx:id="start" alignment="BOTTOM_CENTER" mnemonicParsing="false" onAction="#start" onMousePressed="#mousePressStart" onMouseReleased="#mouseReleaseStart" style="-fx-border-style: none; -fx-border-width: 0; -fx-border-insets: 0; -fx-padding: 1;" />
                           </children>
                        </VBox>
                     </center>
                  </BorderPane>
               </children>
            </StackPane>
         </children>
      </StackPane>
   </children>
</AnchorPane>
