<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="anchor" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.StartScene">
   <children>
      <BorderPane fx:id="borderPane" prefHeight="900.0" prefWidth="1600.0" style="-fx-background-image: url(&quot;../../background.png&quot;);">
         <center>
            <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="50.0" BorderPane.alignment="CENTER">
               <children>
                  <Button mnemonicParsing="false" text="Button" visible="false" />
                  <Button fx:id="start" alignment="BOTTOM_CENTER" mnemonicParsing="false" onAction="#start" onMousePressed="#mousePressStart" onMouseReleased="#mouseReleaseStart" style="-fx-border-style: none; -fx-border-width: 0; -fx-border-insets: 0; -fx-padding: 1;" />
               </children>
            </VBox>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
